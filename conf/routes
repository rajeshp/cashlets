# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page

*       /auth                                   module:securesocial
GET     /                                       staticFile:/public/html/index.html
GET     /{page}.html                            Router.route
GET     /login                                  securesocial.SecureSocial.login

POST   /addService                               Application.AddService

GET    /registerUser                            UserController.registerUser

#GET     /*.jpg                                  staticFile:/public/images/*.jpg




# Ignore favicon requests
GET     /favicon.ico                            404

# Map static resources from the /app/public folder to the /public path
GET     /public/                                staticDir:public

# Catch all
*       /{controller}/{action}                  {controller}.{action}
